{
  "python.pythonPath": "/usr/bin/python",
  "jupyter.jupyterServerType": "local",
  "files.exclude": {
    "backend/**": true,
    "frontend/**": true,
    ".pytest_cache/**": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/result/**": true,
    "**/mlruns/**": true,
    "**/data/**": true,
    "**/*cache*/**": true,
    "**/wk/**": true,
    "**/bk/**": true
  },
  "python.formatting.provider": "black"
}
